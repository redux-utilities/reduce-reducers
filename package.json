{
  "name": "@redux-utilities/reduce-reducers",
  "version": "0.1.4",
  "description": "Reduce multiple reducers into a single reducer",
  "keywords": [
    "reduce",
    "reducers",
    "redux"
  ],
  "license": "MIT",
  "author": "Andrew Clark <acdlite@me.com>",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "repository": "redux-utilities/reduce-reducers",
  "scripts": {
    "precommit": "lint-staged",
    "format": "prettier --write \"{lib,test}/*.{js,md}\"",
    "format:check": "prettier --list-different \"{lib,test}/*.{js,md}\"",
    "lint": "xo",
    "test": "ava",
    "build": "babel src --out-dir=dist"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "prettier": "^1.12.0",
    "xo": "^0.20.3"
  },
  "xo": {
    "prettier": true,
    "space": true
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
